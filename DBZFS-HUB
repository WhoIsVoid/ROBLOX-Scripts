local GC = getconnections or get_signal_cons -- synx / krnl support

if GC then
	for i,v in pairs(GC(Client.Idled)) do
	    if v['Disable'] then
			v['Disable'](v)
		elseif v['Disconnect'] then
			v['Disconnect'](v)
		end
	end
end
