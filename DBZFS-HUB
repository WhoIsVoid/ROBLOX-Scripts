local toggle = sec22:Toggle('Auto Buy Beans (Earth, Heaven)', false, 'Toggle', function(t)
    getgenv().AutoB = t

    while true and task.wait() do
        if not getgenv().AutoB then break end
        Client.PlayerGui.HUD.Bottom.ChatGui.Visible = false
        task.wait(0.015)
        main.NewChat('Korin BEANS')
        task.wait(.3)
        main.RespondChat('k')
        task.wait(.3)
        main.RespondChat('k')
        task.wait(.3)
        main.RespondChat(TYPE)
        task.wait(.3)
        main.RespondChat('k')
        task.wait(.3)
        main.RespondChat(AMT)
        task.wait(.3)
        main.RespondChat('k')
        task.wait(.3)
        main.RespondChat(CLR)
        task.wait(.3)
        main.RespondChat('k')

        if AMT == '80' then 
            task.wait(.3)
            main.RespondChat('Yes')
        elseif AMT ~= '80' then
            task.wait(.3)
            main.RespondChat('Use Zenni')
        end
        
        main.RespondChat('k')
        task.wait(.3)
    end
end)

